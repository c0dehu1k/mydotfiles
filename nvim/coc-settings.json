{
  "languageserver": {
  "golang": {
    "command": "gopls",
    "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
    "filetypes": ["go"]
  }
},
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
},
"explorer.width": 30,
"explorer.icon.enableNerdfont": true,
"explorer.previewAction.onHover": false,
"explorer.keyMappings.global": {
  "<cr>": ["expandable?", "expand", "open"],
  "v": "open:vsplit"
}
}
